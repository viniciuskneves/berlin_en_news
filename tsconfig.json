{
  "compilerOptions": {
    "target": "ES2019", // Node.js v12
    "module": "commonjs", // Node.js
    "outDir": "dist",
    "removeComments": true,
    "strict": true,
    "noImplicitReturns": true,
    "noUnusedParameters": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  }
}
