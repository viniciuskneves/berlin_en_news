{
  "name": "berlin_en_news",
  "version": "1.0.0",
  "description": "Berlin EN News Twitter bot.  It fetches news from https://www.berlin.de/en/news/ and Tweets to @Berlin_EN_News.",
  "keywords": ["berlin", "news", "twitter", "bot"],
  "homepage": "https://github.com/viniciuskneves/berlin_en_news/blob/master/README.md",
  "bugs": "https://github.com/viniciuskneves/berlin_en_news/issues",
  "license": "MIT",
  "author": "Vinicius Kiatkoski Neves <viniciuskneves@icloud.com> (https://twitter.com/viniciuskneves)",
  "contributors": [
    "Leonardo Pavan Rocha <leonardo.rocha.pavan@gmail.com> (https://twitter.com/leopavanrocha)"
  ],
  "repository": "github:viniciuskneves/berlin_en_news",
  "scripts": {
    "start": "node index.js",
    "build": "sam build",
    "predeploy": "npm run build",
    "deploy": "sam deploy --parameter-overrides TwitterApiKey=$TWITTER_API_KEY TwitterApiSecretKey=$TWITTER_API_SECRET_KEY TwitterAccessToken=$TWITTER_ACCESS_TOKEN TwitterAccessTokenSecret=$TWITTER_ACCESS_TOKEN_SECRET"
  },
  "dependencies": {
    "axios": "0.19.2",
    "cheerio": "1.0.0-rc.3",
    "twitter": "1.7.1"
  }
}
